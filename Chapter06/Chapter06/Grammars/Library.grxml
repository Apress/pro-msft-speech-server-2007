      if ((year &lt; 1900) || (year &gt; 2099))
      {
        retVal = "false";
      }
      
      oDate.setFullYear(year); 
      oDate.setDate(1);
      oDate.setMonth(month - 1);
      oDate.setDate(day);
      
      if (day != oDate.getDate())
        retVal = "false";
        
      return retVal;
  }</tag>
		<tag>$.Day = []; $.Month = []; $.Year = []; $.AlternateMonth = []; $.AlternateYear = [];</tag>
		<item repeat="0-1">
