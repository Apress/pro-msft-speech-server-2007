      $._attributes.name = "Explicit";
      $._attributes.form = "Long Form";</tag>
		<one-of>
			<item>
				<item>nineteen hundred</item>
				<tag>$._value = 1900;</tag>
				<item repeat="0-1">
					<item repeat="0-1">and</item>
					<ruleref uri="#Cardinal_1_to_99" type="application/srgs+xml"/>
					<tag>$._value = $._value + $$._value;</tag>
				</item>
			</item>
			<item>
				<item>nineteen</item>
				<tag>$._value = 1900;</tag>
				<one-of>
					<item>
						<ruleref uri="#Cardinal_10_to_99" type="application/srgs+xml"/>
					</item>
					<item>
						<ruleref uri="#Cardinal_0" type="application/srgs+xml"/>
						<ruleref uri="#Cardinal_0_to_9" type="application/srgs+xml"/>
					</item>
				</one-of>
				<tag>$._value = $._value + $$._value;</tag>
			</item>
			<item>
				<item>one nine</item>
				<tag>$._value = 1900;</tag>
				<ruleref uri="#Cardinal_0_to_9" type="application/srgs+xml"/>
				<tag>$._value = $._value + $$._value * 10;</tag>
				<ruleref uri="#Cardinal_0_to_9" type="application/srgs+xml"/>
				<tag>$._value = $._value + $$._value;</tag>
			</item>
			<item>
				<item>two thousand</item>
				<tag>$._value = 2000;</tag>
				<item repeat="0-1">
					<item repeat="0-1">and</item>
					<ruleref uri="#Cardinal_1_to_99" type="application/srgs+xml"/>
					<tag>$._value = $._value + $$._value;</tag>
				</item>
			</item>
			<item>
				<item>two</item>
				<ruleref uri="#Cardinal_0" type="application/srgs+xml"/>
				<tag>$._value = 2000;</tag>
				<ruleref uri="#Cardinal_0_to_9" type="application/srgs+xml"/>
				<tag>$._value = $._value + $$._value * 10;</tag>
				<ruleref uri="#Cardinal_0_to_9" type="application/srgs+xml"/>
				<tag>$._value = $._value + $$._value;</tag>
			</item>
			<item>
				<item>twenty</item>
				<tag>$._value = 2000;</tag>
				<one-of>
					<item>
						<ruleref uri="#Cardinal_10_to_99" type="application/srgs+xml"/>
					</item>
					<item>
						<ruleref uri="#Cardinal_0" type="application/srgs+xml"/>
						<ruleref uri="#Cardinal_0_to_9" type="application/srgs+xml"/>
					</item>
				</one-of>
				<tag>$._value = $._value + $$._value;</tag>
			</item>
			<item>
				<tag>$._attributes.name = "Derived";
          $._attributes.form = "Short Form";</tag>
				<one-of>
					<item>
						<ruleref uri="#Cardinal_0" type="application/srgs+xml"/>
						<ruleref uri="#Cardinal_0_to_9" type="application/srgs+xml"/>
					</item>
					<item>
						<ruleref uri="#Cardinal_10_to_99" type="application/srgs+xml"/>
					</item>
				</one-of>
				<tag>$._value = $$._value;</tag>
				<tag>if ($._value &gt;= 30) {
            $._value = $._value + 1900;
          }
          else {
            $._value = $._value + 2000;
          }</tag>
			</item>
			<item>
				<item>next year</item>
				<tag>$._value = $._value + 1;
