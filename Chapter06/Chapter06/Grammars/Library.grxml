	<!--  Category: Time-->
	<!--  Description: The Time rule provides for input of clock time quantities. -->
	<!--  -->
	<!--  $Time.Hour:-->
	<!--    $Time.Hour._value: integer [0...23]-->
	<!--    $Time.Hour._attributes.name: string ["Assumed", "Explicit"]-->
	<!--  $Time.Minute:-->
	<!--    $Time.Minute: integer [0...59]-->
	<!--  $Time.AlternateHour._value: integer [0...23]-->
	<!--  -->
	<rule id="Time" scope="public">
		<example>twelve a. m.</example>
		<example>seven o'clock</example>
		<example>seven thirty p. m.</example>
		<example>five minutes before six o'clock p. m.</example>
		<example>two past ten</example>
		<example>a quarter before ten</example>
		<example>half past three o'clock p. m.</example>
		<example>three fifteen</example>
		<example>six twenty at night</example>
		<example>ten of five</example>
		<tag>// only allows hours between 1 and 12
  // if ampm is empty then the time is calculated to be am if hour is greater than 7 and pm otherwise
  // returns a value between 0 and 23
  function Hour24(hour, ampm) {
		var retVal = hour;
    
    if ((hour &gt; 12) || (hour &lt; 0))
      return retVal;
      
    if (ampm == "AM")
    {
      if (hour == 12)
        retVal = 0;
    }
    else if (ampm == "PM")
    {
      if (hour &lt; 12)
        retVal = hour + 12;
    }
    else
    {
      if (hour &lt; 7)
        retVal = hour + 12;
    }
    
    return retVal;
  }</tag>
		<tag>// only allows hours between 1 and 12
  // returns a value between 0 and 23
  function AlternateHour24(hour) {
		var retVal = hour;
    
    if ((hour &gt; 12) || (hour &lt; 0))
      return retVal;
      
    if (hour == 12)
      retVal = 0;

    else if (hour &gt; 6)
      retVal = hour + 12;
    
    return retVal;
  }</tag>
		<tag>// only allows offsets between -59 and +59
  function NormalizeHour(hour, offset) {
		var retVal = hour;
    
    if (Math.abs(offset) &gt; 59)
      return hour;
    
    if ((hour &gt; 23) || (hour &lt; 0))
      return hour;
      
    retVal = (offset &lt; 0) ? retVal - 1 : retVal;
    retVal = (retVal &lt; 0) ? 23 : retVal;
    return retVal;
	}</tag>
		<tag>// only allows offsets between -59 and +59
  function NormalizeMinute(offset) {
		var retVal = 0;
    
    if (Math.abs(offset) &gt; 59)
      return 0;
    
    retVal = (offset &lt; 0) ? 60 + offset: offset;
    return retVal;
	}</tag>
		<tag>$.Hour = []; $.Minute = [];</tag>
		<one-of>
			<item>
				<ruleref uri="#Time_Hour" type="application/srgs+xml"/>
				<tag>$ = $$;
          $.Minute = [];
          $.Minute._value = 0;</tag>
			</item>
			<item>
				<ruleref uri="#Time_BeforeAfter" type="application/srgs+xml"/>
				<tag>var offset = $$._value;</tag>
				<ruleref uri="#Time_Hour" type="application/srgs+xml"/>
				<tag>$ = $$;
          $.Minute = [];
          $.Hour._value = NormalizeHour($.Hour._value, offset);
          $.Minute._value = NormalizeMinute(offset);
          if (typeof($.AlternateHour) != "undefined") {
            $.AlternateHour._value = NormalizeHour($.AlternateHour._value, offset);
          }</tag>
			</item>
			<item>
				<ruleref uri="#Time_HourNum" type="application/srgs+xml"/>
				<tag>$.Hour._value = Hour24($$._value, "");
          $.Hour._attributes.name = "Assumed";
          $.AlternateHour = [];
          $.AlternateHour._value = AlternateHour24($$._value);</tag>
				<ruleref uri="#Time_Minute" type="application/srgs+xml"/>
				<tag>$.Minute._value = $$._value;</tag>
			</item>
			<item>
				<ruleref uri="#Time_HourNum" type="application/srgs+xml"/>
				<tag>$.Hour._value = $$._value;
          $.Hour._attributes.name = "Explicit";</tag>
				<ruleref uri="#Time_Minute" type="application/srgs+xml"/>
				<tag>$.Minute._value = $$._value;</tag>
				<ruleref uri="#Time_AmPm" type="application/srgs+xml"/>
				<tag>$.Hour._value = Hour24($.Hour._value, $$._value);</tag>
			</item>
			<item>
				<tag>var offset = 0;
          $.Hour._attributes.name = "Explicit";
          $.Minute._value = 0;</tag>
				<item repeat="0-1">
					<ruleref uri="#Time_BeforeAfter" type="application/srgs+xml"/>
					<tag>offset = $$._value;</tag>
				</item>
				<one-of>
					<item>
						<item>noon</item>
						<tag>$.Hour._value = 12;</tag>
					</item>
					<item>
						<item>midnight</item>
						<tag>$.Hour._value = 0;</tag>
					</item>
				</one-of>
				<tag>$.Hour._value = NormalizeHour($.Hour._value, offset);
          $.Minute._value = NormalizeMinute(offset);</tag>
			</item>
		</one-of>
	</rule>
